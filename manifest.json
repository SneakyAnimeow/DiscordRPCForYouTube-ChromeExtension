{
    "name": "YouTube Discord RPC",
    "version": "1.0",
    "manifest_version": 3,
    "description": "YouTube Discord RPC updater",
    "icons": {
      "16": "icons/vanced16x16.png",
      "32": "icons/vanced32x32.png",
      "48": "icons/vanced48x48.png",
      "128": "icons/vanced128x128.png"
    },
    "permissions": ["scripting", "tabs", "webRequest", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "index.js"
    }
}